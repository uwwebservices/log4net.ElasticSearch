<?xml version="1.0"?>
<package >
    <metadata>
        <id>log4net.ElasticSearch-AWS</id>
        <version>2.3.4.7</version>
        <authors>UW Web Services Team</authors>
        <owners>UW Web Services Team</owners>
        <iconUrl>http://nest.azurewebsites.net/images/nuget-icon.png</iconUrl>
        <licenseUrl>https://github.com/jptoto/log4net.ElasticSearch/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/jptoto/log4net.ElasticSearch</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>This package exteneds the log4net.Elasticsearch adapter to support logging to AWS based Elasticsearch instances by providing a way to provide IAM credentials and sign the http calls to AWS using the Amazon Version 4 Signing Process.</description>
        <releaseNotes>Add the AWS specific code. Add a UW specific event log class. Change to .NET 4.5.1 version. Clean-up various readme notes.</releaseNotes>
        <copyright>Copyright UW 2017</copyright>
        <tags>log logging exception elasticsearch log4net aws</tags>
        <dependencies>
            <group targetFramework="net451">
                <dependency id="log4net" version="2.0.5.0" />
            </group>
            <group targetFramework="net40">
                <dependency id="log4net" version="2.0.5.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="lib\**\log4net.ElasticSearch*.dll" target="lib"/>
        <file src="lib\**\log4net.ElasticSearch*.pdb" target="lib"/>
        <file src="lib\**\log4net.ElasticSearch*.XML" target="lib"/>
        <file src="readme.txt" target="" />
    </files>
</package>
