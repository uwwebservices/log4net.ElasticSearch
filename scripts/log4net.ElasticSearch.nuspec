<?xml version="1.0"?>
<package >
    <metadata>
        <id>log4net.ElasticSearch-AWS</id>
        <version>2.3.4.1</version>
        <authors>UW Web Services Team</authors>
        <owners>UW Web Services Team</owners>
        <iconUrl>http://nest.azurewebsites.net/images/nuget-icon.png</iconUrl>
        <licenseUrl>https://github.com/jptoto/log4net.ElasticSearch/blob/master/LICENSE</licenseUrl>
        <projectUrl>https://github.com/jptoto/log4net.ElasticSearch</projectUrl>
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>log4net.Elasticsearch is a log4net adapter for easy logging of exceptions and messages to Elasticsearch indices. Tested through Elasticsearch 5.3.0. This version has been extended to support logging to AWS based elasticsearch instance</description>
        <releaseNotes>Modify Content-Type header to propertly use application/json so the ES deprecatio log doesn't complain.</releaseNotes>
        <copyright>Copyright 2017</copyright>
        <tags>log logging exception elasticsearch log4net</tags>
        <dependencies>
            <group targetFramework="net45">
                <dependency id="log4net" version="2.0.3.0" />
            </group>
            <group targetFramework="net40">
                <dependency id="log4net" version="2.0.3.0" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="lib\**\log4net.ElasticSearch*.dll" target="lib"/>
        <file src="lib\**\log4net.ElasticSearch*.pdb" target="lib"/>
        <file src="lib\**\log4net.ElasticSearch*.XML" target="lib"/>
        <file src="readme.txt" target="" />
    </files>
</package>
